{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient.WebStoreWebAPI": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      },
